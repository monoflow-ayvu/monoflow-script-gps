{
  "name": "gps",
  "version": "0.17.0",
  "description": "Script para GPS. Tracking GPS, geolocalización, geocercas, etc.",
  "settingsSchema": {
    "display": "form",
    "components": [
      {
        "label": "Velocidade límite (0 = nenha velocidade límite)",
        "tableView": false,
        "suffix": "KM/H",
        "mask": false,
        "defaultValue": 0,
        "delimiter": false,
        "requireDecimal": false,
        "inputFormat": "plain",
        "truncateMultipleSpaces": false,
        "validate": {
          "required": true
        },
        "key": "speedLimit",
        "type": "number",
        "conditional": {
          "show": true,
          "when": "saveGPS",
          "eq": "true"
        },
        "input": true
      },
      {
        "label": "Salvar posições GPS?",
        "tableView": false,
        "defaultValue": true,
        "key": "saveGPS",
        "type": "checkbox",
        "input": true
      },
      {
        "label": "Cada quantos minutos salvar posições? (0 = salvar sempre)",
        "tableView": false,
        "suffix": "Minutos",
        "mask": false,
        "defaultValue": 0,
        "delimiter": false,
        "requireDecimal": false,
        "inputFormat": "plain",
        "truncateMultipleSpaces": false,
        "validate": {
          "required": true
        },
        "key": "saveEveryMins",
        "type": "number",
        "conditional": {
          "show": true,
          "when": "saveGPS",
          "eq": "true"
        },
        "input": true
      },
      {
        "label": "Habilitar geocercas?",
        "tableView": false,
        "key": "enableGeofences",
        "type": "checkbox",
        "input": true,
        "defaultValue": false
      },
      {
        "label": "Geocercas",
        "reorder": false,
        "addAnotherPosition": "bottom",
        "layoutFixed": false,
        "enableRowGroups": false,
        "initEmpty": false,
        "tableView": false,
        "defaultValue": [
          {
            "name": "",
            "kind": "default",
            "wkt": ""
          }
        ],
        "key": "geofences",
        "conditional": {
          "show": true,
          "when": "enableGeofences",
          "eq": "true"
        },
        "type": "datagrid",
        "input": true,
        "components": [
          {
            "label": "Nome",
            "tableView": true,
            "validate": {
              "required": true
            },
            "key": "name",
            "type": "textfield",
            "input": true
          },
          {
            "label": "Tipo",
            "widget": "choicesjs",
            "tableView": true,
            "defaultValue": "default",
            "data": {
              "values": [
                {
                  "label": "Padrão",
                  "value": "default"
                },
                {
                  "label": "Velocidade Límite",
                  "value": "speedLimit"
                }
              ]
            },
            "validate": {
              "required": true
            },
            "key": "kind",
            "type": "select",
            "case": "lowercase",
            "input": true
          },
          {
            "label": "Geocerca WKT",
            "placeholder": "POLYGON(...",
            "tooltip": "Geocerca no formato WKT (Well-Known Text)",
            "autoExpand": false,
            "tableView": true,
            "validate": {
              "custom": "valid = /^POLYGON(\\s+)?\\(.*\\)$/.test(input) ? true : 'Geocerca inválida';"
            },
            "key": "wkt",
            "type": "textarea",
            "input": true
          },
          {
            "label": "Velocidade límite",
            "suffix": "KM/H",
            "mask": false,
            "tableView": false,
            "delimiter": false,
            "requireDecimal": false,
            "inputFormat": "plain",
            "truncateMultipleSpaces": false,
            "validate": {
              "required": true
            },
            "key": "speedLimit",
            "conditional": {
              "show": true,
              "when": "geofences.kind",
              "eq": "speedLimit"
            },
            "type": "number",
            "input": true
          }
        ]
      },
      {
        "type": "button",
        "label": "Submit",
        "key": "submit",
        "disableOnInvalid": true,
        "input": true,
        "tableView": false
      }
    ]
  },
  "license": "EULA",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --mode production --watch",
    "build-copy": "pnpm build && (cat dist/bundle.js | pbcopy) && du -sh dist/*.js",
    "size": "npm run build && size-limit",
    "deploy": "node .scripts/deploy.js",
    "deploy-force": "node .scripts/deploy.js --force",
    "minor": "npm run build && bump minor --commit --tag --push --all",
    "release": "npm run build && bump major --commit --tag --push --all",
    "test": "jest"
  },
  "browserslist": [
    "ie 6"
  ],
  "size-limit": [
    {
      "path": "dist/bundle.js",
      "limit": "100 kb",
      "gzip": false
    }
  ],
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/plugin-transform-arrow-functions": "7.16.7",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-typescript": "7.16.7",
    "@jsdevtools/version-bump-prompt": "6.1.0",
    "@size-limit/preset-big-lib": "7.0.8",
    "@types/commander": "2.12.2",
    "@types/firebase": "3.2.1",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.182",
    "@types/uuid": "8.3.4",
    "@types/wellknown": "0.5.2",
    "babel-loader": "8.2.5",
    "brotli-webpack-plugin": "1.1.0",
    "commander": "9.2.0",
    "compression-webpack-plugin": "9.2.0",
    "graphql": "16.4.0",
    "graphql-request": "4.2.0",
    "inquirer": "8.2.4",
    "jest": "28.0.3",
    "mobx-keystone": "0.68.3",
    "semver": "7.3.7",
    "size-limit": "7.0.8",
    "ts-loader": "9.3.0",
    "typed-emitter": "2.1.0",
    "typescript": "4.6.4",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2"
  },
  "dependencies": {
    "@fermuch/monoutils": "0.1.4",
    "@fermuch/telematree": "9.21.0",
    "cross-fetch": "3.1.5",
    "firebase": "9.7.0",
    "geo-point-in-polygon": "1.0.0",
    "lodash": "4.17.21",
    "mobx": "6.5.0",
    "wellknown": "0.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/private/jest/setup.ts"
    ]
  }
}
